!(構造-具型別 '映射 !(構造-序列* !(構造-序列*
  !(構造-序列* '空 !(構造-具型別 '映射 !(構造-序列* !(構造-序列*))))
  !(構造-序列* '連 !(遞歸 $連 !(函式 ($甲:映射 $乙:映射) !(連-映射 $甲:映射 $乙:映射))))
  !(構造-序列* '取 !(函式 ($:映射 $甲) !(解算 $:映射 !(構造-具型別 '式 !(構造-序列* '識別子 !(構造-序列* $甲))))))
  !(構造-序列* '置 !(函式 ($:映射 $鍵 $值) !(連-映射 !(構造-具型別 '映射 !(構造-序列* !(構造-序列* !(構造-序列* $鍵 $值)))) $:映射)))
)))
